{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.js"
        }
    ],
    "env": {
        "JWT_SECRET": "@jwt_secret",
        "MONGODB_HOST": "@mongodb_host"
    }
}
